{
  "name": "bit-webspace-vue",
  "version": "6.0.0-SNAPSHOT",
  "private": true,
  "license": "MIT",
  "scripts": {
    "composer:install": "composer install",
    "composer:updatedry": "composer update --dry-run",
    "composer:update": "composer update",
    "dev": "vite",
    "symfony": "symfony server:start",
    "prebuild": "composer dump-env prod",
    "build": "run-p type-check \"build-only {@}\" --",
    "build-only": "vite build",
    "postbuild": "APP_ENV=prod composer install --no-dev --optimize-autoloader",
    "test:unit": "vitest --environment jsdom --dir src/vue/",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "engines": {
    "node": ">=20.9",
    "npm": ">=10.1.0",
    "pnpm": ">=8.10.2"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.2",
    "vue": "^3.3.8",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.5.1",
    "@tsconfig/node20": "^20.1.2",
    "@types/bootstrap": "^5.2.9",
    "@types/jsdom": "^21.1.5",
    "@types/node": "^20.9.0",
    "@vitejs/plugin-vue": "^4.4.1",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.53.0",
    "eslint-plugin-vue": "^9.18.1",
    "jsdom": "^22.1.0",
    "npm-run-all2": "^6.1.1",
    "sass": "^1.69.5",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-symfony": "^6.0.1",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.22"
  }
}
