{
  "name": "bit-webspace-vue",
  "version": "6.0.0-SNAPSHOT",
  "private": true,
  "license": "MIT",
  "scripts": {
    "composer:install": "composer install",
    "composer:updatedry": "composer update --dry-run",
    "composer:update": "composer update",
    "dev": "vite",
    "symfony": "symfony server:start",
    "prebuild": "composer dump-env prod",
    "build": "run-p type-check \"build-only {@}\" --",
    "build-only": "vite build",
    "postbuild": "APP_ENV=prod composer install --no-dev --optimize-autoloader",
    "test:unit": "vitest --environment jsdom --dir src/vue/",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "engines": {
    "node": ">=20.10",
    "npm": ">=10.2.5",
    "pnpm": ">=8.12.1"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.2",
    "vue": "^3.4.4",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.6.1",
    "@tsconfig/node20": "^20.1.2",
    "@types/bootstrap": "^5.2.10",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.10.6",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.2",
    "jsdom": "^23.0.1",
    "npm-run-all2": "^6.1.1",
    "sass": "^1.69.7",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-symfony": "^6.3.2",
    "vitest": "^1.1.1",
    "vue-tsc": "^1.8.27"
  }
}
