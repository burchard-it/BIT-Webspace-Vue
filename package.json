{
  "name": "bit-webspace-vue",
  "version": "6.0.0-SNAPSHOT",
  "private": true,
  "license": "MIT",
  "scripts": {
    "composer:install": "composer install",
    "composer:updatedry": "composer update --dry-run",
    "composer:update": "composer update",
    "dev": "vite",
    "symfony": "symfony server:start",
    "prebuild": "composer dump-env prod",
    "build": "run-p type-check build-only",
    "build-only": "vite build",
    "postbuild": "APP_ENV=prod composer install --no-dev --optimize-autoloader",
    "test:unit": "vitest --environment jsdom --dir src/vue/",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "engines": {
    "node": ">=18.12",
    "npm": ">=9.2"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "bootstrap": "^5.2.3",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/bootstrap": "^5.2.6",
    "@types/jsdom": "^21.1.0",
    "@types/node": "^18.13.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.2.10",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.33.0",
    "eslint-plugin-vue": "^9.9.0",
    "jsdom": "^21.1.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.58.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.5",
    "vite-plugin-symfony": "^0.7.5",
    "vitest": "^0.28.4",
    "vue-tsc": "^1.0.24"
  }
}
